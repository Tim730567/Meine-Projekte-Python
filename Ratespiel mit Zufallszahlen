from random import randint 
zahl = randint(1,100)

# Programm implementieren womit ich eine Zahl erraten kann, wo gefragt wird was ich denke was die Zahl ist.
print("Hey ich habe mir eine zahl zwischen 1 und 100 ausgedacht:)!")
while True:
    versuch = int(input("Rate meine Zahl: "))
    if versuch == zahl:
        print("Super du hast gewonnen!") 
        break
    elif versuch < zahl:
        print("Die Zahl ist zu klein!")
    else: 
        print("Die Zahl ist zu groÃŸ!")
